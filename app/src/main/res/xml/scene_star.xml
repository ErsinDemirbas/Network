<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">


    <Transition
        motion:constraintSetStart="@+id/btn_start"
        motion:constraintSetEnd="@+id/end"
        motion:duration="5000"
        motion:motionInterpolator = "lineer">
        <OnClick
            motion:touchAnchorId="@+id/pc1"
            motion:clickAction="toggle" />

    </Transition>




    <ConstraintSet android:id="@+id/btn_start">
            <Constraint
            android:id="@+id/pc1"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">

          </Constraint>

        <Constraint
        android:id="@+id/pc2"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="8dp"
        motion:layout_constraintBottom_toBottomOf="parent"
        motion:layout_constraintStart_toStartOf="parent"
        motion:layout_constraintTop_toTopOf="parent">
    </Constraint>

        <Constraint
            android:id="@+id/pc3"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="18dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

        <Constraint
            android:id="@+id/pc4"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="295dp"
            android:layout_marginBottom="28dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

        <Constraint
            android:id="@+id/pc5"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="295dp"
            android:layout_marginBottom="8dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

        <Constraint
            android:id="@+id/hub1"
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:layout_marginStart="104dp"

            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>
    </ConstraintSet>


    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/pc1"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginEnd="158dp"
            android:layout_marginBottom="107dp"

            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

           <Constraint
            android:id="@+id/pc2"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginEnd="227dp"
            android:layout_marginBottom="30dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

        <Constraint
            android:id="@+id/pc3"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginEnd="190dp"
            android:layout_marginTop="10dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

        <Constraint
            android:id="@+id/pc4"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginEnd="118dp"
            android:layout_marginTop="8dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

        <Constraint
            android:id="@+id/pc5"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginEnd="90dp"
            android:layout_marginBottom="24dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

        <Constraint
            android:id="@+id/hub1"
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:layout_marginEnd="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
        </Constraint>

    </ConstraintSet>





    <KeyFrameSet>
        <KeyCycle
            motion:framePosition="0"
            motion:motionTarget="@id/pc1"
            android:translationY= "50dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="sin" />

        <KeyCycle
            motion:framePosition="30"
            motion:motionTarget="@id/pc1"
            android:translationY= "50dp"
            motion:wavePeriod="1"
            motion:waveOffset="0"
            motion:waveShape="sin"/>

        <KeyCycle
            motion:framePosition="100"
            motion:motionTarget="@id/pc1"
            android:translationY= "50dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="sin"/>

        <KeyCycle
            motion:framePosition="0"
            motion:motionTarget="@id/pc2"
            android:translationY= "50dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="sin" />

        <KeyCycle
            motion:framePosition="50"
            motion:motionTarget="@id/pc2"
            android:translationY= "75dp"
            motion:wavePeriod="1"
            motion:waveOffset="0"
            motion:waveShape="sin"/>


        <KeyCycle
            motion:framePosition="100"
            motion:motionTarget="@id/pc2"
            android:translationY= "50dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="sin"/>

        <KeyCycle
        motion:framePosition="0"
        motion:motionTarget="@id/pc3"
        android:translationY= "10dp"
        motion:wavePeriod="0"
        motion:waveOffset="0"
        motion:waveShape="cos" />

        <KeyCycle
            motion:framePosition="30"
            motion:motionTarget="@id/pc3"
            android:translationY= "50dp"
            motion:wavePeriod="1"
            motion:waveOffset="0"
            motion:waveShape="cos"/>

        <KeyCycle
            motion:framePosition="100"
            motion:motionTarget="@id/pc3"
            android:translationY= "50dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="cos"/>

        <KeyCycle
            motion:framePosition="0"
            motion:motionTarget="@id/pc4"
            android:translationY= "10dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="cos" />

        <KeyCycle
            motion:framePosition="30"
            motion:motionTarget="@id/pc4"
            android:translationY= "50dp"
            motion:wavePeriod="1"
            motion:waveOffset="0"
            motion:waveShape="cos"/>

        <KeyCycle
            motion:framePosition="100"
            motion:motionTarget="@id/pc4"
            android:translationY= "50dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="cos"/>

        <KeyCycle
            motion:framePosition="0"
            motion:motionTarget="@id/pc5"
            android:translationY= "50dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="sin" />

        <KeyCycle
            motion:framePosition="50"
            motion:motionTarget="@id/pc5"
            android:translationY= "75dp"
            motion:wavePeriod="1"
            motion:waveOffset="0"
            motion:waveShape="sin"/>


        <KeyCycle
            motion:framePosition="100"
            motion:motionTarget="@id/pc5"
            android:translationY= "50dp"
            motion:wavePeriod="0"
            motion:waveOffset="0"
            motion:waveShape="sin"/>



    </KeyFrameSet>

</MotionScene>